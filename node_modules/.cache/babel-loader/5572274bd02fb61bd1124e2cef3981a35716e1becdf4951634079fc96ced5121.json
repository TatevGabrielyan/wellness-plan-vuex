{"ast":null,"code":"import { ref, watch, nextTick } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  name: 'To-do',\n  props: ['app'],\n  setup(props) {\n    const input = ref(null);\n    const editing = ref(false);\n    watch(editing, v => {\n      v && nextTick(() => {\n        input.value.focus();\n      });\n    });\n    const store = useStore();\n    const editApp = (app, value) => store.dispatch('editApp', {\n      app,\n      value\n    });\n    const toggleApp = app => store.dispatch('toggleApp', app);\n    const removeApp = app => store.dispatch('removeApp', app);\n    function doneEdit(e) {\n      const value = e.target.value.trim();\n      if (!value) {\n        removeApp(props.app);\n      } else if (editing.value) {\n        editApp(props.app, value);\n      }\n      editing.value = false;\n    }\n    function cancelEdit(e) {\n      e.target.value = props.app.text;\n      editing.value = false;\n    }\n    return {\n      input,\n      editing,\n      toggleApp,\n      doneEdit,\n      cancelEdit,\n      removeApp\n    };\n  }\n};","map":{"version":3,"mappings":"AAgCA,SAASA,GAAG,EAAEC,KAAK,EAAEC,QAAO,QAAS,KAAI;AACzC,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAK,EAAE,CAAC,KAAK,CAAC;EAEdC,KAAI,CAAGD,KAAK,EAAE;IACZ,MAAME,KAAI,GAAIP,GAAG,CAAC,IAAI;IAEtB,MAAMQ,OAAM,GAAIR,GAAG,CAAC,KAAK;IAEzBC,KAAK,CAACO,OAAO,EAAGC,CAAC,IAAK;MACpBA,KAAKP,QAAQ,CAAC,MAAM;QAAEK,KAAK,CAACG,KAAK,CAACC,KAAK,EAAC;MAAE,CAAC;IAC7C,CAAC;IAED,MAAMC,KAAI,GAAIT,QAAQ,EAAC;IACvB,MAAMU,OAAM,GAAI,CAACC,GAAG,EAAEJ,KAAK,KAAKE,KAAK,CAACG,QAAQ,CAAC,SAAS,EAAE;MAAED,GAAG;MAAEJ;IAAM,CAAC;IACxE,MAAMM,SAAQ,GAAKF,GAAG,IAAKF,KAAK,CAACG,QAAQ,CAAC,WAAW,EAAED,GAAG;IAC1D,MAAMG,SAAQ,GAAKH,GAAG,IAAKF,KAAK,CAACG,QAAQ,CAAC,WAAW,EAAED,GAAG;IAE1D,SAASI,QAAO,CAAGC,CAAC,EAAE;MACpB,MAAMT,KAAI,GAAIS,CAAC,CAACC,MAAM,CAACV,KAAK,CAACW,IAAI,EAAC;MAClC,IAAI,CAACX,KAAK,EAAE;QACVO,SAAS,CAACZ,KAAK,CAACS,GAAG;MACrB,OAAO,IAAIN,OAAO,CAACE,KAAK,EAAE;QACxBG,OAAO,CAACR,KAAK,CAACS,GAAG,EAAEJ,KAAK