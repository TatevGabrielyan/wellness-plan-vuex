{"ast":null,"code":"import { mapState, mapActions } from 'vuex';\nimport { currency } from '../currency';\nexport default {\n  computed: mapState({\n    products: state => state.products.all\n  }),\n  methods: {\n    ...mapActions('cart', ['addProductToCart']),\n    currency\n  },\n  created() {\n    this.$store.dispatch('products/getAllProducts');\n  }\n};","map":{"version":3,"mappings":"AAiBA,SAASA,QAAQ,EAAEC,UAAS,QAAS,MAAK;AAC1C,SAASC,QAAO,QAAS,aAAY;AAErC,eAAe;EACbC,QAAQ,EAAEH,QAAQ,CAAC;IACjBI,QAAQ,EAAEC,KAAI,IAAKA,KAAK,CAACD,QAAQ,CAACE;EACpC,CAAC,CAAC;EACFC,OAAO,EAAE;IACP,GAAGN,UAAU,CAAC,MAAM,EAAE,CACpB,kBAAiB,CAClB,CAAC;IACFC;EACF,CAAC;EACDM,OAAM,GAAK;IACT,IAAI,CAACC,MAAM,CAACC,QAAQ,CAAC,yBAAyB;EAChD;AACF","names":["mapState","mapActions","currency","computed","products","state","all","methods","created","$store","dispatch"],"sourceRoot":"","sources":["C:\\Users\\User\\vue-chart\\src\\components\\ProductList.vue"],"sourcesContent":["<template>\n  <ul>\n    <li\n      v-for=\"product in products\"\n      :key=\"product.id\">\n      {{ product.title }} - {{ currency(product.price) }}\n      <br>\n      <button\n        :disabled=\"!product.inventory\"\n        @click=\"addProductToCart(product)\">\n        Add to cart\n      </button>\n    </li>\n  </ul>\n</template>\n\n<script>\nimport { mapState, mapActions } from 'vuex'\nimport { currency } from '../currency'\n\nexport default {\n  computed: mapState({\n    products: state => state.products.all\n  }),\n  methods: {\n    ...mapActions('cart', [\n      'addProductToCart'\n    ]),\n    currency\n  },\n  created () {\n    this.$store.dispatch('products/getAllProducts')\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}