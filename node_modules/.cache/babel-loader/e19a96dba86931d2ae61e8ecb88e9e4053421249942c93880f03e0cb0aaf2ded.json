{"ast":null,"code":"import { nextTick } from 'vue';\nimport { mapActions } from 'vuex';\nexport default {\n  name: 'Todo',\n  props: ['todo'],\n  data() {\n    return {\n      editing: false\n    };\n  },\n  directives: {\n    focus(el, {\n      value\n    }, {\n      context\n    }) {\n      if (value) {\n        context.$nextTick(() => {\n          el.focus();\n        });\n      }\n    }\n  },\n  watch: {\n    editing(v) {\n      v && nextTick(() => {\n        this.$refs.input.focus();\n      });\n    }\n  },\n  methods: {\n    ...mapActions(['editTodo', 'toggleTodo', 'removeTodo']),\n    doneEdit(e) {\n      const value = e.target.value.trim();\n      const {\n        todo\n      } = this;\n      if (!value) {\n        this.removeTodo(todo);\n      } else if (this.editing) {\n        this.editTodo({\n          todo,\n          value\n        });\n        this.editing = false;\n      }\n    },\n    cancelEdit(e) {\n      e.target.value = this.todo.text;\n      this.editing = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAqBA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,UAAS,QAAS,MAAK;AAEhC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,KAAK,EAAE,CAAC,MAAM,CAAC;EACfC,IAAG,GAAK;IACN,OAAO;MACLC,OAAO,EAAE;IACX;EACF,CAAC;EACDC,UAAU,EAAE;IACVC,KAAI,CAAGC,EAAE,EAAE;MAAEC;IAAM,CAAC,EAAE;MAAEC;IAAQ,CAAC,EAAE;MACjC,IAAID,KAAK,EAAE;QACTC,OAAO,CAACC,SAAS,CAAC,MAAM;UACtBH,EAAE,CAACD,KAAK,EAAC;QACX,CAAC;MACH;IACF;EACF,CAAC;EACDK,KAAK,EAAE;IACLP,OAAM,CAAGQ,CAAC,EAAE;MACVA,KAAKb,QAAQ,CAAC,MAAM;QAAE,IAAI,CAACc,KAAK,CAACC,KAAK,CAACR,KAAK,EAAC;MAAE,CAAC;IAClD;EACF,CAAC;EACDS,OAAO,EAAE;IACP,GAAGf,UAAU,CAAC,CACZ,UAAU,EACV,YAAY,EACZ,YAAW,CACZ,CAAC;IACFgB,QAAO,CAAGC,CAAC,EAAE;MACX,MAAMT,KAAI,GAAIS,CAAC,CAACC,MAAM,CAACV,KAAK,CAACW,IAAI,EAAC;MAClC,MAAM;QAAEC;MAAK,IAAI,IAAG;MACpB,IAAI,CAACZ,KAAK,EAAE;QACV,IAAI,CAACa,UAAU,CAACD,IAAI;MACtB,OAAO,IAAI,IAAI,CAAChB,OAAO,EAAE;QACvB,IAAI,CAACkB,QAAQ,CAAC;UACZF,IAAI;UACJZ;QACF,CAAC;QACD,IAAI,CAACJ,OAAM,GAAI,KAAI;MACrB;IACF,CAAC;IACDmB,UAAS,CAAGN,CAAC,EAAE;MACbA,CA