 filters[nov.value](todos.value));\n    const filteredDec = computed(() => filters[dec.value](todos.value));\n    const remaining = computed(() => todos.value.filter(todo => !todo.done).length);\n    const toggleAll = done => store.dispatch('toggleAll', done);\n    function addTodo(e) {\n      const text = e.target.value;\n      if (text.trim()) {\n        store.dispatch('addTodo', text);\n      }\n      e.target.value = '';\n    }\n    const pluralize = (n, w) => n === 1 ? w : w + 's';\n    const capitalize = s => s.charAt(0).toUpperCase() + s.slice(1);\n    return {\n      filters,\n      todos,\n      allChecked,\n      remaining,\n      filteredJan,\n      filteredFeb,\n      filteredMarch,\n      filteredApril,\n      filteredMay,\n      filteredJune,\n      filteredJuly,\n      filteredAug,\n      filteredSep,\n      filteredOct,\n      filteredNov,\n      filteredDec,\n      addTodo,\n      toggleAll,\n      pluralize,\n      capitalize\n    };\n  }\n};","map":{"version":3,"mappings":"AA+DA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,QAAO,MAAO,gBAAe;AAEpC,MAAMC,OAAM,GAAI;EACdC,GAAG,EAAEC,KAAI,IAAKA,KAAK;EACnBC,GAAG,EAAED,KAAI,IAAKA,KAAK;EACnBE,GAAG,EAAEF,KAAI,IAAKA,KAAK;EACnBG,GAAG,EAAEH,KAAI,IAAKA,KAAK;EACnBI,GAAG,EAAEJ,KAAI,IAAKA,KAAK;EACnBK,IAAI,EAAEL,KAAI,IAAKA,KAAK;EACpBM,IAAI,EAAEN,KAAI,IAAKA,KAAK;EACpBO,GAAG,EAAEP,KAAI,IAAKA,KAAK;EACnBQ,GAAG,EAAER,KAAI,IAAKA,KAAK;EACnBS,GAAG,EAAET,KAAI,IAAKA,KAAK;EACnBU,GAAG,EAAEV,KAAI,IAAKA,KAAK;EACnBW,GAAG,EAAEX,KAAI,IAAKA,KAAK;EACnBY,MAAM,EAAEZ,KAAI,IAAKA,KAAK,CAACa,MAAM,CAACC,IAAG,IAAK,CAACA,IAAI,CAACC,IAAI;AAClD;AAEA,eAAe;EACbC,UAAU,EAAE;IAACpB,YAAY;IAAEC;EAAS,CAAC;EACrCoB,IAAI,GAAE;IACJ,OAAM;MACJC,IAAI,EAAC;IACP;EACF,CAAC;EACDC,KAAI,GAAK;IACP,MAAMC,GAAE,GAAI3B,GAAG,CAAC,KAAK;IACrB,MAAM4B,GAAE,GAAI5B,GAAG,CAAC,KAAK;IACrB,MAAM6B,KAAI,GAAI7B,GAAG,CAAC,KAAK;IACvB,MAAM8B,KAAI,GAAI9B,GAAG,CAAC,KAAK;IACvB,MAAM+B,GAAE,GAAI/B,GAAG,CAAC,KAAK;IACrB,MAAMgC,IAAG,GAAIhC,G