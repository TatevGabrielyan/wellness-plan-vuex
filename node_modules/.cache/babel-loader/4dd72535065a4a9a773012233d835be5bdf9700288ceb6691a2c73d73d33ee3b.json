](apps.value));\n    const filteredDec = computed(() => filters[dec.value](apps.value));\n    const remaining = computed(() => apps.value.filter(app => !app.done).length);\n    const toggleAll = done => store.dispatch('toggleAll', done);\n    function addApp(e) {\n      const text = e.target.value;\n      if (text.trim()) {\n        store.dispatch('addApp', text);\n      }\n      e.target.value = '';\n    }\n    const pluralize = (n, w) => n === 1 ? w : w + 's';\n    const capitalize = s => s.charAt(0).toUpperCase() + s.slice(1);\n    return {\n      filters,\n      apps,\n      allChecked,\n      remaining,\n      filteredJan,\n      filteredFeb,\n      filteredMarch,\n      filteredApril,\n      filteredMay,\n      filteredJune,\n      filteredJuly,\n      filteredAug,\n      filteredSep,\n      filteredOct,\n      filteredNov,\n      filteredDec,\n      addApp,\n      toggleAll,\n      pluralize,\n      capitalize\n    };\n  }\n};","map":{"version":3,"mappings":"AA2DA,SAASA,GAAG,EAAEC,QAAO,QAAS,KAAI;AAClC,SAASC,QAAO,QAAS,MAAK;AAC9B,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,OAAM,MAAO,eAAc;AAElC,MAAMC,OAAM,GAAI;EACdC,GAAG,EAAEC,IAAG,IAAKA,IAAI;EACjBC,GAAG,EAAED,IAAG,IAAKA,IAAI,CAACE,MAAM,CAACC,GAAE,IAAK,CAACA,GAAG,CAAC,CAAC,CAAC,CAAC;EACxCC,GAAG,EAAEJ,IAAG,IAAKA,IAAI;EACjBK,GAAG,EAAEL,IAAG,IAAKA,IAAI;EACjBM,GAAG,EAAEN,IAAG,IAAKA,IAAI;EACjBO,IAAI,EAAEP,IAAG,IAAKA,IAAI;EAClBQ,IAAI,EAAER,IAAG,IAAKA,IAAI;EAClBS,GAAG,EAAET,IAAG,IAAKA,IAAI;EACjBU,GAAG,EAAEV,IAAG,IAAKA,IAAI;EACjBW,GAAG,EAAEX,IAAG,IAAKA,IAAI;EACjBY,GAAG,EAAEZ,IAAG,IAAKA,IAAI;EACjBa,GAAG,EAAEb,IAAG,IAAKA;EACb;AACF;;AAEA,eAAe;EACbc,UAAU,EAAE;IAAClB,YAAY;IAAEC;EAAQ,CAAC;EACpCkB,IAAI,GAAE;IACJ,OAAM;MACJC,IAAI,EAAC;IACP;EACF,CAAC;EACDC,KAAI,GAAK;IACP,MAAMC,GAAE,GAAIzB,GAAG,CAAC,KAAK;IACrB,MAAM0B,GAAE,GAAI1B,GAAG,CAAC,KAAK;IACrB,MAAM2B,KAAI,GAAI3B,GAAG,CAAC,KAAK;IACvB,MAAM4B,KAAI,GAAI5B,GAAG,CAAC,KAAK;IACvB,MAAM6B,GAAE,GAAI7B,GAAG,CAAC,KAAK;IACrB,MAAM8B,IAAG,GAAI9B,GAAG,CAAC,MAAM;IACvB,MAAM+B,IAAG,GAAI/B,GAAG,CAAC,MAAM